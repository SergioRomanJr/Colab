        "# Pie Chart"
      ],
      "metadata": {
        "id": "61Srs9a8xCQn"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "\n",
        "labels = ['frogs','dogs','cats','birds']\n",
        "sizes = [15,30,45,10]\n",
        "explode =(0,0.1,0,0)\n",
        "\n",
        "fig1, ax1= plt.subplots()\n",
        "\n",
        "ax1.pie(sizes,\n",
        "        explode=explode,\n",
        "        labels=labels,\n",
        "        autopct= '%1.1f%%',\n",
        "        shadow=True,\n",
        "        startangle=90)\n",
        "ax1.axis('equal')\n"
      ],
      "metadata": {
        "id": "EZ12Rg_Y27Dj",
